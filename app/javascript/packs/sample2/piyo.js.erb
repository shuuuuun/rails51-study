import rails_png from '../images/rails.png'

<% helpers = ActionController::Base.helpers %>
var railsImagePath = "<%= helpers.image_path('rails.jpg') %>";
var assetPackPath = "<%= helpers.asset_pack_path('rails.png') %>";

createImg(railsImagePath)
createImg(assetPackPath)
createImg(rails_png)

console.log('sample2/piyo')
console.log('railsImagePath: ', railsImagePath)
console.log('assetPackPath: ', assetPackPath)
console.log('rails_png: ', rails_png)

function createImg(src) {
  var img = document.createElement('img');
  img.src = src;
  document.body.appendChild(img);
}
